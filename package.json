{
    "name": "weedio",
    "version": "1.0.0",
    "description": "Command to run index.js file:",
    "main": "index.js",
    "scripts": {
        "start": "node --experimental-modules --es-module-specifier-resolution=node ./lib/index.js",
        "lt": "lt --port 3000 --subdomain weedio",
        "dev": "tsc && npm start",
        "build:css": "postcss public/stylesheets/tailwind.css -o public/stylesheets/style.css"
    },
    "author": "akshitdandyan",
    "license": "MIT",
    "dependencies": {
        "@ffmpeg-installer/ffmpeg": "^1.1.0",
        "autoprefixer": "^10.4.13",
        "axios": "^1.1.3",
        "chalk": "^4.1.2",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "ffmpeg": "^0.0.4",
        "fluent-ffmpeg": "^2.1.2",
        "localtunnel": "^2.0.2",
        "node-telegram-bot-api": "^0.60.0",
        "postcss": "^8.4.20",
        "postcss-cli": "^10.1.0",
        "tailwindcss": "^3.2.4"
    },
    "devDependencies": {
        "@types/chalk": "^2.2.0",
        "@types/express": "^4.17.15",
        "@types/ffmpeg": "^1.0.4",
        "@types/fluent-ffmpeg": "^2.1.20",
        "@types/node-telegram-bot-api": "^0.57.6",
        "typescript": "^4.9.4"
    }
}
